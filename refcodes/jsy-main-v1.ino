// Verso otimizada do drawHeader
void drawHeader() {
  // Limpar a rea do cabealho
  tft.fillRect(0, 0, tft.width(), 55, COLOR_BACKGROUND);
  
  // PowerGard Monitor no topo
  tft.setTextColor(COLOR_GAS_ALARM_BG, COLOR_BACKGROUND);
  tft.drawString("PowerGard Monitor", 10, 10, 2);
  
  // Status MQTT
  if (client.connected()) {
    tft.setTextColor(COLOR_VOLTAGE, COLOR_BACKGROUND);
    tft.drawString("MQTT: OK", tft.width() - 80, 10, 2);
  } else {
    tft.setTextColor(COLOR_WARNING, COLOR_BACKGROUND);
    tft.drawString("MQTT: NC", tft.width() - 80, 10, 2);
  }
  
  // Linha separadora
  tft.drawFastHLine(0, 30, tft.width(), COLOR_TITLE);
  
  // Mostra a hora
  tft.setTextColor(COLOR_TEXT, COLOR_BACKGROUND);
  tft.drawString(sensorData.timeStr, 10, 35, 2);
  
  // Indicador de fora do sinal WiFi com barras
  int rssi = WiFi.RSSI();
  tft.drawString("WiFi:", tft.width() - 100, 35, 2);
  
  // Posio das barras
  int barX = tft.width() - 50;
  int barY = 37;
  int barWidth = 3;  // Largura de cada barra
  int barGap = 2;    // Espao entre as barras
  int maxBars = 5;   // Nmero total de barras
  
  // Determina quantas barras mostrar baseado no RSSI
  int activeBars;
  if (rssi >= -50) activeBars = 5;      // -50 dBm ou melhor (Excelente)
  else if (rssi >= -60) activeBars = 4;  // -60 dBm (Muito Bom)
  else if (rssi >= -70) activeBars = 3;  // -70 dBm (Bom)
  else if (rssi >= -80) activeBars = 2;  // -80 dBm (Regular)
  else activeBars = 1;                   // Pior que -80 dBm (Fraco)
  
  // Desenha as barras
  for (int i = 0; i < maxBars; i++) {
    int barHeight = 4 + (i * 2);  // Cada barra fica um pouco mais alta
    int currentBarY = barY + (12 - barHeight);  // Alinha as barras na parte inferior
    
    // Desenha barra ativa ou inativa
    if (i < activeBars) {
      tft.fillRect(barX + (i * (barWidth + barGap)), currentBarY, barWidth, barHeight, COLOR_VOLTAGE);
    } else {
      tft.fillRect(barX + (i * (barWidth + barGap)), currentBarY, barWidth, barHeight, COLOR_INACTIVE);
    }
  }
  
  // Linha separadora aps o tempo e WiFi
  tft.drawFastHLine(0, 55, tft.width(), COLOR_TITLE);
}

// Verso otimizada do updateDisplay
void updateDisplay() {
  // Desenha o cabealho
  drawHeader();
  
  // Limpa a rea de contedo
  tft.fillRect(0, 56, tft.width(), tft.height() - 56, COLOR_BACKGROUND);
  
  // Se o alarme estiver ativo, mostra a pgina de alarme apropriada
  if (alarmActive && activeAlarmPage >= 0) {
    switch (activeAlarmPage) {
      case 0: // Alarme de perda de fase
        drawPhaseFailAlarmPage();
        break;
      case 1: // Alarme de tenso
        drawVoltageAlarmPage();
        break;
      case 2: // Alarme de gs
        drawGasAlarmPage();
        break;
    }
  } else {
    // Mostra pginas regulares
    switch(currentPage) {
      case 0:
        drawPage1(); // Dados da Fase 1
        break;
      case 1:
        drawPage2(); // Dados da Fase 2
        break;
      case 2:
        drawPage3(); // Dados da Fase 3
        break;
      case 3:
        drawPage4(); // Status do sistema
        break;
      case 4:
        drawPage5(); // Pgina de resumo
        break;
    }
    
    // Desenha o indicador de pgina
    drawPageIndicator();
  }
}

