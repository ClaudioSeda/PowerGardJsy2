// Desenha o indicador de pgina na parte inferior
void drawPageIndicator() {
  // Coordenadas Y absolutas na tela
  int indicatorY = tft.height() - 15;
  
  // Limpa a rea do indicador de pgina
  tft.fillRect(0, tft.height() - 25, tft.width(), 25, COLOR_BACKGROUND);
  
  int dotSize = 5;
  int dotSpacing = 15;
  int startX = (tft.width() - ((TOTAL_PAGES - 1) * dotSpacing + dotSize)) / 2;
  
  for (int i = 0; i < TOTAL_PAGES; i++) {
    if (i == currentPage) {
      tft.fillCircle(startX + i * dotSpacing, indicatorY, dotSize, COLOR_TITLE);
    } else {
      tft.drawCircle(startX + i * dotSpacing, indicatorY, dotSize, COLOR_TEXT);
    }
  }
  
  // Adiciona setas de navegao
  tft.fillTriangle(10, indicatorY, 5, indicatorY - 5, 5, indicatorY + 5, COLOR_TEXT); // Seta esquerda
  tft.fillTriangle(tft.width() - 10, indicatorY, tft.width() - 5, indicatorY - 5, tft.width() - 5, indicatorY + 5, COLOR_TEXT); // Seta direita
}

// Atualiza hora e WiFi
void updateTimeAndWifi() {
  struct tm timeinfo;
  if (getLocalTime(&timeinfo)) {
    strftime(sensorData.timeStr, sizeof(sensorData.timeStr), "%H:%M:%S", &timeinfo);
  }
  
  sensorData.wifiStrength = WiFi.RSSI();
}

