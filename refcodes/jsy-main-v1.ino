void handleButtons() {
  // Read current button states
  bool upState = digitalRead(BUTTON_UP_PIN);
  bool downState = digitalRead(BUTTON_DOWN_PIN);
  unsigned long currentTime = millis();
  
  // Handle UP button with debounce
  if (upState != lastUpState && currentTime - lastUpDebounceTime > DEBOUNCE_TIME) {
    lastUpDebounceTime = currentTime;
    
    // Button is pressed (LOW when pressed with INPUT_PULLUP)
    if (upState == LOW) {
      // If alarm is active, pressing any button acknowledges it
      if (alarmActive) {
        // Just acknowledge the alarm, the system will show another alarm if present
        // or return to normal pages
        resetAlarms();
        Serial.println("Alarm acknowledged via UP button");
      } else {
        // Navigate to previous page
        currentPage = (currentPage > 0) ? currentPage - 1 : TOTAL_PAGES - 1;
        Serial.print("Button: Previous page - now showing page ");
        Serial.println(currentPage + 1);
        needsRefresh = true;
      }
    }
    
    lastUpState = upState;
  }
  
  // Handle DOWN button with debounce
  if (downState != lastDownState && currentTime - lastDownDebounceTime > DEBOUNCE_TIME) {
    lastDownDebounceTime = currentTime;
    
    // Button is pressed (LOW when pressed with INPUT_PULLUP)
    if (downState == LOW) {
      // If alarm is active, pressing any button acknowledges it
      if (alarmActive) {
        // Just acknowledge the alarm, the system will show another alarm if present
        // or return to normal pages
        resetAlarms();
        Serial.println("Alarm acknowledged via DOWN button");
      } else {
        // Navigate to next page
        currentPage = (currentPage + 1) % TOTAL_PAGES;
        Serial.print("Button: Next page - now showing page ");
        Serial.println(currentPage + 1);
        needsRefresh = true;
      }
    }
    
    lastDownState = downState;
  }
}

void callback(char* topic, byte* payload, unsigned int length) {
  StaticJsonDocument<768> doc;
  
  char buffer[length + 1];
  memcpy(buffer, payload, length);
  buffer[length] = '\0';
  
  Serial.print("Received MQTT message: ");
  Serial.println(buffer);
  
  DeserializationError error = deserializeJson(doc, buffer);
  
  if (!error) {
    // Extract legacy values
    sensorData.gasmon = doc["gm"] | 0;
    sensorData.overSub = doc["os"] | 0;
    sensorData.faseStatus = doc["fs"] | 0;
    sensorData.activeStatus = doc["as"] | 0;
    sensorData.levCx1 = doc["cx1"] | 0;
    sensorData.levCx2 = doc["cx2"] | 0;
    sensorData.levCs1 = doc["cs1"] | 0;
    
    // Extract Phase A (1) values
    JsonObject faseA = doc["fase1"];
    if (faseA) {
      sensorData.phase1.voltage = faseA["v"];
      sensorData.phase1.current = faseA["c"];
      sensorData.phase1.power = faseA["p"];
      sensorData.phase1.energy = faseA["e"];
      sensorData.phase1.powerFactor = faseA["pf"];
    }
    
    // Extract Phase B (2) values
    JsonObject faseB = doc["fase2"];
    if (faseB) {
      sensorData.phase2.voltage = faseB["v"];
      sensorData.phase2.current = faseB["c"];
      sensorData.phase2.power = faseB["p"];
      sensorData.phase2.energy = faseB["e"];
      sensorData.phase2.powerFactor = faseB["pf"];
    }
    
    // Extract Phase C (3) values
    JsonObject faseC = doc["fase3"];
    if (faseC) {
      sensorData.phase3.voltage = faseC["v"];
      sensorData.phase3.current = faseC["c"];
      sensorData.phase3.power = faseC["p"];
      sensorData.phase3.energy = faseC["e"];
      sensorData.phase3.powerFactor = faseC["pf"];
    }
    
    sensorData.lastUpdate = millis();
    needsRefresh = true;
    Serial.println("Data updated from MQTT");
  } else {
    Serial.print("JSON parsing error: ");
    Serial.println(error.c_str());
  }
}

void reconnect() {
  int attempts = 0;
  while (!client.connected() && attempts < 3) {  // Limit reconnection attempts
    attempts++;
    Serial.print("Attempting MQTT connection... ");
    String clientId = "ESP32Client-";
    clientId += String(random(0xffff), HEX);
    
    if (client.connect(clientId.c_str(), MQTT_USERNAME, MQTT_PASSWORD)) {
      Serial.println("connected");
      client.subscribe(MQTT_TOPIC);
    } else {
      Serial.print("failed, rc=");
      Serial.print(client.state());
      Serial.println(" retrying in 5 seconds");
      delay(5000);
    }
  }
}

