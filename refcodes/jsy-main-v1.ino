void loop() {
  if (!client.connected()) {
    reconnect();
  }
  client.loop();
  
  // Check for alarms periodically
  unsigned long currentMillis = millis();
  if (currentMillis - lastAlarmCheck > ALARM_CHECK_INTERVAL) {
    checkAlarms();
    lastAlarmCheck = currentMillis;
  }
  
  // Check physical buttons
  handleButtons();
  
  // Check serial commands
  handleSerialCommands();
  
  if (needsRefresh) {
    updateDisplay();
    needsRefresh = false;
  }
  
  // Update time and wifi indicator periodically
  static unsigned long lastTimeUpdate = 0;
  if (millis() - lastTimeUpdate > 1000) {
    updateTimeAndWifi();
    drawHeader();
    lastTimeUpdate = millis();
  }
  
  delay(50);
}

void checkAlarms() {
  bool previousAlarmState = alarmActive;
  int previousActiveAlarm = activeAlarmPage;
  
  // Reset alarm states
  alarmStates.voltageAlarm = false;
  alarmStates.phaseAlarm = false;
  alarmStates.gasAlarm = false;
  
  // Fill alarm info with current values
  
  // Check for voltage alarms
  alarmStates.voltageInfo.phase1High = sensorData.phase1.voltage > VOLTAGE_HIGH_THRESHOLD;
  alarmStates.voltageInfo.phase1Low = sensorData.phase1.voltage < VOLTAGE_LOW_THRESHOLD && sensorData.phase1.voltage >= PHASE_LOSS_THRESHOLD;
  alarmStates.voltageInfo.phase2High = sensorData.phase2.voltage > VOLTAGE_HIGH_THRESHOLD;
  alarmStates.voltageInfo.phase2Low = sensorData.phase2.voltage < VOLTAGE_LOW_THRESHOLD && sensorData.phase2.voltage >= PHASE_LOSS_THRESHOLD;
  alarmStates.voltageInfo.phase3High = sensorData.phase3.voltage > VOLTAGE_HIGH_THRESHOLD;
  alarmStates.voltageInfo.phase3Low = sensorData.phase3.voltage < VOLTAGE_LOW_THRESHOLD && sensorData.phase3.voltage >= PHASE_LOSS_THRESHOLD;
  
  // Check for phase loss
  alarmStates.phaseInfo.phase1Loss = sensorData.phase1.voltage < PHASE_LOSS_THRESHOLD;
  alarmStates.phaseInfo.phase2Loss = sensorData.phase2.voltage < PHASE_LOSS_THRESHOLD;
  alarmStates.phaseInfo.phase3Loss = sensorData.phase3.voltage < PHASE_LOSS_THRESHOLD;
  
  // Set main alarm flags
  alarmStates.voltageAlarm = alarmStates.voltageInfo.phase1High || alarmStates.voltageInfo.phase1Low || 
                            alarmStates.voltageInfo.phase2High || alarmStates.voltageInfo.phase2Low || 
                            alarmStates.voltageInfo.phase3High || alarmStates.voltageInfo.phase3Low;
                            
  alarmStates.phaseAlarm = alarmStates.phaseInfo.phase1Loss || 
                          alarmStates.phaseInfo.phase2Loss || 
                          alarmStates.phaseInfo.phase3Loss;
                          
  alarmStates.gasAlarm = sensorData.gasmon > GAS_ALARM_THRESHOLD;
  
  // Determine which alarm to show (priority: phase loss > voltage > gas)
  if (alarmStates.phaseAlarm) {
    alarmActive = true;
    activeAlarmPage = 0; // Phase alarm page
  } else if (alarmStates.voltageAlarm) {
    alarmActive = true;
    activeAlarmPage = 1; // Voltage alarm page
  } else if (alarmStates.gasAlarm) {
    alarmActive = true;
    activeAlarmPage = 2; // Gas alarm page
  } else {
    alarmActive = false;
    activeAlarmPage = -1;
  }
  
  // If alarm state or active alarm changed, refresh display
  if (previousAlarmState != alarmActive || previousActiveAlarm != activeAlarmPage) {
    needsRefresh = true;
    
    if (alarmActive) {
      Serial.print("ALARME ATIVO: ");
      switch (activeAlarmPage) {
        case 0:
          Serial.println("Falha de Fase");
          break;
        case 1:
          Serial.println("Tenso Anormal");
          break;
        case 2:
          Serial.println("Vazamento de Gs");
          break;
      }
    } else if (previousAlarmState) {
      Serial.println("Alarmes normalizados");
    }
  }
}

void resetAlarms() {
  alarmActive = false;
  activeAlarmPage = -1;
  needsRefresh = true;
}

void handleSerialCommands() {
  // Process any incoming serial data
  while (Serial.available() > 0) {
    char inChar = (char)Serial.read();
    
    // Process the character
    if (inChar == '\n' || inChar == '\r') {
      if (serialCmd.length() > 0) {
        serialCmdComplete = true;
      }
    } else {
      serialCmd += inChar;
    }
  }
  
  // Process the completed command
  if (serialCmdComplete) {
    processSerialCommand();
    serialCmd = "";
    serialCmdComplete = false;
  }
}

void processSerialCommand() {
  serialCmd.trim();
  
  // Convert to lowercase for case-insensitive commands
  serialCmd.toLowerCase();
  
  if (serialCmd == "help" || serialCmd == "h") {
    showHelp();
  } 
  else if (serialCmd == "next" || serialCmd == "n") {
    // Go to next page (only if no alarm is active)
    if (!alarmActive) {
      currentPage = (currentPage + 1) % TOTAL_PAGES;
      Serial.print("Showing page ");
      Serial.print(currentPage + 1);
      Serial.print(" of ");
      Serial.println(TOTAL_PAGES);
      needsRefresh = true;
    } else {
      Serial.println("Cannot change page while alarm is active");
    }
  }
  else if (serialCmd == "prev" || serialCmd == "p") {
    // Go to previous page (only if no alarm is active)
    if (!alarmActive) {
      currentPage = (currentPage > 0) ? currentPage - 1 : TOTAL_PAGES - 1;
      Serial.print("Showing page ");
      Serial.print(currentPage + 1);
      Serial.print(" of ");
      Serial.println(TOTAL_PAGES);
      needsRefresh = true;
    } else {
      Serial.println("Cannot change page while alarm is active");
    }
  }
  else if (serialCmd.startsWith("page ")) {
    // Go to specific page, e.g., "page 1" (only if no alarm is active)
    if (!alarmActive) {
      int page = serialCmd.substring(5).toInt();
      if (page >= 1 && page <= TOTAL_PAGES) {
        currentPage = page - 1;
        Serial.print("Showing page ");
        Serial.println(page);
        needsRefresh = true;
      } else {
        Serial.print("Invalid page number. Please enter 1-");
        Serial.println(TOTAL_PAGES);
      }
    } else {
      Serial.println("Cannot change page while alarm is active");
    }
  }
  else if (serialCmd == "status") {
    // Show system status
    Serial.println("\nSystem Status:");
    Serial.print("Current Page: ");
    if (alarmActive) {
      Serial.print("ALARM - ");
      switch (activeAlarmPage) {
        case 0: Serial.println("Phase Failure"); break;
        case 1: Serial.println("Voltage Abnormal"); break;
        case 2: Serial.println("Gas Leak"); break;
      }
    } else {
      Serial.print(currentPage + 1);
      Serial.print("/");
      Serial.println(TOTAL_PAGES);
    }
    Serial.print("WiFi RSSI: ");
    Serial.print(WiFi.RSSI());
    Serial.println(" dBm");
    Serial.print("MQTT Status: ");
    Serial.println(client.connected() ? "Connected" : "Disconnected");
    
    // Print alarm states
    Serial.println("\nAlarm States:");
    Serial.print("Voltage Alarm: ");
    Serial.println(alarmStates.voltageAlarm ? "ACTIVE" : "Inactive");
    Serial.print("Phase Alarm: ");
    Serial.println(alarmStates.phaseAlarm ? "ACTIVE" : "Inactive");
    Serial.print("Gas Alarm: ");
    Serial.println(alarmStates.gasAlarm ? "ACTIVE" : "Inactive");
  }
  else if (serialCmd == "testdata") {
    // Generate test data
    simulateData(true);
    Serial.println("Generated random test data");
    needsRefresh = true;
  }
  else if (serialCmd == "zerodata") {
    // Reset all data to zero
    simulateData(false);
    Serial.println("Reset all data to zero");
    needsRefresh = true;
  }
  else if (serialCmd == "testvoltage") {
    // Generate voltage alarm data
    simulateData(false);
    sensorData.phase1.voltage = 90.0; // Under voltage
    sensorData.phase2.voltage = 140.0; // Over voltage
    sensorData.phase3.voltage = 120.0; // Normal voltage
    Serial.println("Generated voltage alarm test data");
    needsRefresh = true;
  }
  else if (serialCmd == "testphase") {
    // Generate phase loss alarm data
    simulateData(false);
    sensorData.phase1.voltage = 0.0; // Phase loss
    sensorData.phase2.voltage = 120.0; // Normal
    sensorData.phase3.voltage = 5.0; // Phase loss
    Serial.println("Generated phase loss test data");
    needsRefresh = true;
  }
  else if (serialCmd == "testgas") {
    // Generate gas alarm data
    simulateData(false);
    sensorData.gasmon = 12; // Gas alarm
    Serial.println("Generated gas alarm test data");
    needsRefresh = true;
  }
  else if (serialCmd == "resetalarms") {
    // Reset all alarms (for testing purposes)
    resetAlarms();
    Serial.println("Reset all alarms");
  }
  else if (serialCmd == "refresh" || serialCmd == "r") {
    // Force refresh display
    needsRefresh = true;
    Serial.println("Refreshing display");
  }
  else {
    Serial.print("Unknown command: '");
    Serial.print(serialCmd);
    Serial.println("'. Type 'help' for available commands.");
  }
}

